<% form_for(@wiki_page, :url => wiki_pages_path) do |form| %>
  <%= render :partial => form %>

  <div class="operations">
    <%= form.submit t('wiki_engine.create') %> |
    <%= preview_button %> |
    <%= link_to t('wiki_engine.cancel'), wiki_pages_path %>
  </div>
<% end %>
<div id="preview"></div>
