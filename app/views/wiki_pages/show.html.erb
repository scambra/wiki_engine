<%= render :partial => 'wiki_page', :object => @version %>

<div class="operations">
  <%= link_to t('wiki_engine.edit'), edit_wiki_page_path(:id => @wiki_page) %> |
  <%= button_to t('wiki_engine.delete'), wiki_page_path(:id => @wiki_page), :method => :delete, :confirm => t('wiki_engine.confirm_deletion') %> |
  <%= link_to t('wiki_engine.main_page'), wiki_pages_path %>
  <%= select_tag :versions, options_for_versions_select(@versions), :onchange => 'document.location.href = this.value;' %>
  <%= t('wiki_engine.last_modified_with_time', :time => l(@version.created_at)) %>
</div>
